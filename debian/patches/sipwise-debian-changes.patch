+        id         CDATA  #REQUIRED
+        hours      CDATA  #IMPLIED
+        monthdays  CDATA  #IMPLIED
+        weekdays   CDATA  #IMPLIED
+        yeardays   CDATA  #IMPLIED
+        months     CDATA  #IMPLIED
+        weeks      CDATA  #IMPLIED
+        weekyears  CDATA  #IMPLIED
+        years      CDATA  #IMPLIED
+        moon       CDATA  #IMPLIED>
+
